{
	"manifest_version": 2,
	"name": "Override examples",
	"version": "0.1",

	"icons" : {
		"48" : "icons/beasts-48.png"
	},

	"permissions": [
		"topSites", "bookmarks"
	],
	"background": {
		"scripts": ["/js/listener.js"]
	},

	"browser_action" : {
		"default_icon" : "icons/beasts-32.png",
		"default_title" : "file manager"
	},

	"web_accessible_resources" : [
		"data/fileManagerData.js",
		"data/images/filemanager/*.png"
	]

	"applications": {
		"gecko": {
			"strict_min_version": "54.0a1"
		}
	}
}
