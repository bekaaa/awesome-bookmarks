{
	"manifest_version": 2,
	"name": "Awesome Bookmarks",
	"version": "0.1",

	"icons" : {
		"48" : "icons/logo-48.png"
	},

	"permissions": [
		"bookmarks"
	],
	"background": {
		"scripts": ["/js/listener.js"]
	},

	"browser_action" : {
		"default_icon" : "icons/logo-32.png",
		"default_title" : "AwBookmarks"
	},

	"web_accessible_resources" : [
		"/index.html",
		"/js/*.js",
		"/icons/*.png"
	]

}
